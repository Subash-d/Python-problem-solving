    "class Solution:\n",
    "    def findClosest(self, x: int, y: int, z: int) -> int:\n",
    "        dif_xz = abs(x-z)\n",
    "        dif_yz = abs(y-z)\n",
    "        return 1 if dif_xz < dif_yz else 2 if dif_yz < dif_xz else 0 \n",
    "    \n",
    "solution = Solution()\n",
    "solution.findClosest(3,7,5)"
   ]
